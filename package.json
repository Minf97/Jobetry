  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "fix",
          "docs",
          "style",
          "refactor",
          "perf",
          "test",
          "chore",
          "revert",
          "build",
          "ci"
        ]
      ],
      "type-case": [
        2,
        "always",
        "lower-case"
      ],
      "type-empty": [
        2,
        "never"
      ],
      "scope-empty": [
        0
      ],
      "scope-case": [
        0
      ],
      "subject-full-stop": [
        0
      ],
      "subject-case": [
        0
      ],
      "header-max-length": [
        2,
        "always",
        100
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  }
